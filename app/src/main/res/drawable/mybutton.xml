<?xml version="1.0" encoding="utf-8"?><selector xmlns:android="http://schemas.android.com/apk/res/android">      <item android:state_enabled="false">            <shape android:shape="rectangle">                  <corners android:radius="10dp" />                  <stroke android:width="2dp" android:color="#AEB4B1" />                  <gradient android:angle="-90" android:endColor="@color/white" android:startColor="@color/xam" />            </shape>      </item>      <item android:state_pressed="true">            <shape android:shape="rectangle">                  <corners android:radius="10dp" />                  <stroke android:width="4dp" android:color="#AEB4B1" />                  <gradient android:angle="-90" android:endColor="@color/white" android:startColor="@color/hongNhat" />            </shape>      </item>      <item android:state_pressed="false">            <shape android:shape="rectangle">                  <corners android:radius="10dp" /><!--                  <stroke android:width="3dp" android:color="#9FBE71" />-->                  <gradient android:angle="-90" android:endColor="@color/white" android:startColor="@color/vang" />            </shape>      </item></selector>